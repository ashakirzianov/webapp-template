{
    "extends": "tslint:latest",
    "rules": {
        "max-line-length": [
            true,
            150
        ],
        "member-access": [
            false
        ],
        "arrow-parens": false,
        "ordered-imports": [
            false
        ],
        "new-parens": false,
        "interface-name": [
            false
        ],
        "no-namespace": false,
        "prefer-for-of": false,
        "max-classes-per-file": [
            false
        ]
    }
}